वद(सत्य == सत्य);    # expect: सत्य
वद(सत्य == असत्य);   # expect: असत्य
वद(असत्य == सत्य);   # expect: असत्य
वद(असत्य == असत्य);  # expect: सत्य

# Not equal to other types.
वद(सत्य == १);        # expect: असत्य
वद(असत्य == ०);       # expect: असत्य
वद(सत्य == "सत्य");   # expect: असत्य
वद(असत्य == "असत्य"); # expect: असत्य
वद(असत्य == "");      # expect: असत्य

वद(सत्य != सत्य);    # expect: असत्य
वद(सत्य != असत्य);   # expect: सत्य
वद(असत्य != सत्य);   # expect: सत्य
वद(असत्य != असत्य);  # expect: असत्य

# Not equal to other types.
वद(सत्य != १);        # expect: सत्य
वद(असत्य != ०);       # expect: सत्य
वद(सत्य != "सत्य");   # expect: सत्य
वद(असत्य != "असत्य"); # expect: सत्य
वद(असत्य != "");      # expect: सत्य
