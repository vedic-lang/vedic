सूत्र f०() { फल ०; }
वद(f०()); # expect: ०

सूत्र f१(a) { फल a; }
वद(f१(१)); # expect: १

सूत्र f२(a, b) { फल a + b; }
वद(f२(१, २)); # expect: ३

सूत्र f३(a, b, c) { फल a + b + c; }
वद(f३(१, २, ३)); # expect: ६

सूत्र f४(a, b, c, d) { फल a + b + c + d; }
वद(f४(१, २, ३, ४)); # expect: १०

सूत्र f५(a, b, c, d, e) { फल a + b + c + d + e; }
वद(f५(१, २, ३, ४, ५)); # expect: १५

सूत्र f६(a, b, c, d, e, f) { फल a + b + c + d + e + f; }
वद(f६(१, २, ३, ४, ५, ६)); # expect: २१

सूत्र f७(a, b, c, d, e, f, g) { फल a + b + c + d + e + f + g; }
वद(f७(१, २, ३, ४, ५, ६, ७)); # expect: २८

सूत्र f८(a, b, c, d, e, f, g, h) { फल a + b + c + d + e + f + g + h; }
वद(f८(१, २, ३, ४, ५, ६, ७, ८)); # expect: ३६
